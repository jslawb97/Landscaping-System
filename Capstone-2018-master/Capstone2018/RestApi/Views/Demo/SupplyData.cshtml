@model RestApi.Models.ApiResponse<RestApi.Models.Resupply.ApiResupplyOrders>

@{
    ViewBag.Title = "Supply Data Demo";
    Layout = "/Views/Shared/_Layout.cshtml";
}
@if (Model == null)
{
    <p>You have not requested any data</p>
}
else if (Model.Success)
{
    <h3>@Model.Message</h3>
    if (Model.Data != null)
    {
        <p>@Model.Data.ToString()</p>
    }
}
else
{
    <h3>There was a problem processing your request</h3>
    <p>@Model.Message</p>
}
<br/>
@Html.ActionLink("Back to request form", "SupplyRequest")