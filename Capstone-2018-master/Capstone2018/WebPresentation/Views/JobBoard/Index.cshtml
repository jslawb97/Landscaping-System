@model IEnumerable<WebPresentation.Models.EmployeeJobPostViewModel>


<div class="container">
	<table class="table">
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Employee)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.DisplayJobLocationAddress)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.JobScheduled)
			</th>
			<th></th>
		</tr>

		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.Employee)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.DisplayJobLocationAddress)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.JobScheduled)
				</td>
				<td>
					@Html.ActionLink("Accept Job", "Accept", new { id = item.EmployeeJobPostID })
				</td>
			</tr>
		}

	</table>
</div>

